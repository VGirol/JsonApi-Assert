{
    "name": "vgirol/jsonapi",
    "description": "RESTfull JSON:Api",
    "type": "library",
    "homepage": "http://github.com/VGirol/JsonApi",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "spatie/laravel-query-builder": "dev-master",
        "spatie/laravel-json-api-paginate": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "league/flysystem-vfs": "^1.0.0",
        "mockery/mockery": "^1.0",
        "mohammedmanssour/form-request-tester": "dev-master",
        "orchestra/testbench": "~3.7.0",
        "phpunit/phpunit": "^7.0"
    },
    "authors": [
        {
            "name": "Vincent Girol",
            "email": "vincent@girol.fr"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "VGirol\\JsonApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VGirol\\JsonApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "VGirol\\JsonApi\\JsonApiServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
